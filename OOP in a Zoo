function sleep(name) {
    console.log(name + " sleeps for 8 hours");
}

function run() {
    sleep("Tigger");
}

run();

var favoriteFood = "bacon"

function eat(name, food) {
    console.log(name + " eats " + food);
    if (food == favoriteFood) {
        console.log ("YUM!!! " + name + " wants more " + food);
} else { 
    sleep(name);
}
}

eat("Tigger", "meat");
eat("Tigger", "bacon");

